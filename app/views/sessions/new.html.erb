<div class="padding">
    <h1>Sign In</h1>

    <%= form_with url:session_path,local: true do |f| %>
        <br>
        <div>
            <%= f.label :email  %>
            <%= f.email_field :email %> 
        </div>
        <div>
            <%= f.label :password  %> 
            <%= f.password_field :password  %> 
        </div>
        <br>
        <button class="btn btn-outline-primary" type="submit">Sign In</button>
    <% end %>
</div>